<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.composetutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTutorial">
        <activity
            android:name=".BasicLayoutSample"
            android:exported="true"
            android:label="@string/title_activity_basic_layout_sample"
            android:theme="@style/Theme.ComposeTutorial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DefaultComposeActivity"
            android:exported="false"
            android:label="@string/title_activity_compose_sample_list"
            android:theme="@style/Theme.ComposeTutorial" />
        <activity
            android:name=".ComposeSampleList"
            android:exported="true"
            android:label="@string/title_activity_default_compose"
            android:theme="@style/Theme.ComposeTutorial">
        </activity>
    </application>

</manifest>